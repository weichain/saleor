          ]
        },
        "lines": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/TaxLineData"
          }
        }
      },
      "required": [
        "shipping_price_gross_amount",
        "shipping_price_net_amount",
        "shipping_tax_rate",
        "lines"
      ]
    },
    "ShippingMethodSchema": {
      "title": "ShippingMethodSchema",
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "amount": {
          "type": [
            "number",
            "string"
          ]
        },
        "currency": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "maximum_delivery_days": {
          "type": "integer"
        }
      },
      "required": [
